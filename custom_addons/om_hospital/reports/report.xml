<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <!-- QWeb Reports -->
        <!-- NAME & FILE_NAME should be the same -->
            <report
                id="report_patient_card"
                string ="Patient Card"
                model="hospital.patient"
                report_type="qweb-pdf"
                menu="True"
                name="om_hospital.report_patient"
                file="om_hospital.report_patient"
            />

            <report
                id="report_patient_card_xls"
                string ="Patient Card Excel"
                model="hospital.patient"
                report_type="xlsx"
                name="om_hospital.report_patient_xls"
                file="om_hospital.report_patient_xls"
            />

            <report
                id="report_appointment"
                model="hospital.appointment"
                report_type="qweb-pdf"
                menu="True"
                string="appointment report"
                name="om_hospital.report_appointment_tmp"
                file="om_hospital.report_appointment_tmp"
            />

    </data>
</odoo>